{
  "name": "messenger",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint \"./src/**/*.@(ts)\"",
    "lint:fix": "eslint \"./src/**/*.@(ts)\" --fix",
    "stylelint": "stylelint \"**/*.{css,scss,less}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss,less}\" --fix",
    "format:check": "prettier --check \"**/*.{d.ts,ts,js,css,scss,less}\"",
    "format": "prettier --write \"**/*.{d.ts,ts,tsx,js,css,scss,less}\"",
    "start": "vite build && vite preview --port 3000",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky && husky install"
  },
  "lint-staged": {
    "*.ts": "npx tsc --noEmit",
    "*.{js,jsx,ts,tsx}": "npx eslint",
    "*.{js,jsx,ts,tsx,html,css}": "npx prettier --check"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@jest/globals": "29.7.0",
    "@testing-library/jest-dom": "6.4.2",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "29.5.12",
    "@types/jest-sinon": "^1.0.5",
    "@types/node": "^20.14.10",
    "@types/sinon": "^17.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.25.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "3.4.1",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-import-order-all": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "express-handlebars": "^7.1.3",
    "globals": "^15.9.0",
    "husky": "^7.0.4",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-location-mock": "^2.0.0",
    "jest-sinon": "^1.1.0",
    "jest-transform-stub": "2.0.0",
    "jest-watch-typeahead": "2.2.2",
    "jsdom": "^25.0.0",
    "lint-staged": "^15.2.9",
    "path": "^0.12.7",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "sinon": "^18.0.0",
    "stylelint": "^16.8.1",
    "stylelint-config-standard": "^36.0.1",
    "ts-jest": "29.1.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.3.1",
    "vite-plugin-handlebars": "^2.0.0",
    "vite-plugin-inspect": "^0.8.4"
  },
  "dependencies": {
    "@types/handlebars": "^4.1.0",
    "classnames": "^2.5.1",
    "express-handlebars": "^7.1.3",
    "handlebars": "^4.7.8",
    "moment": "^2.30.1",
    "uuid": "^10.0.0",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
